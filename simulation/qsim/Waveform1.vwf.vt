`timescale 1 ps/ 1 ps
module wof_vlg_sample_tst(
	btn,
	FPGA_CLK,
	sampler_tx
);
input  btn;
input  FPGA_CLK;
output sampler_tx;

reg sample;
time current_time;
always @(btn or FPGA_CLK)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

