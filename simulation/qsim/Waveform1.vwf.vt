module wof_vlg_vec_tst();
// constants                                           
// general purpose registers
reg btn;
reg FPGA_CLK;
// wires                                               
wire [7:0] led_out;
wire [6:0] ssd_h;
wire [6:0] ssd_t;
wire [6:0] ssd_th;
wire [6:0] ssd_u;

wire sampler;                             

// assign statements (if any)                          
wof i1 (
// port map - connection between master ports and signals/registers   
	.btn(btn),
	.FPGA_CLK(FPGA_CLK),
	.led_out(led_out),
	.ssd_h(ssd_h),
	.ssd_t(ssd_t),
	.ssd_th(ssd_th),
	.ssd_u(ssd_u)
);

// btn
initial
begin
	btn = 1'b0;
	btn = #100000 1'b1;
	btn = #20000 1'b0;
	btn = #180000 1'b1;
	btn = #10000 1'b0;
end 

wof_vlg_sample_tst tb_sample (
	.btn(btn),
	.FPGA_CLK(FPGA_CLK),
	.sampler_tx(sampler)
);

wof_vlg_check_tst tb_out(
	.led_out(led_out),
	.ssd_h(ssd_h),
	.ssd_t(ssd_t),
	.ssd_th(ssd_th),
	.ssd_u(ssd_u),
	.sampler_rx(sampler)
);
endmodule

